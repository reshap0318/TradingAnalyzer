{
  "name": "saham-analyzer",
  "version": "1.0.0",
  "type": "module",
  "description": "Multi-timeframe stock technical analysis with IHSG correlation",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "start:dns": "node --dns-result-order=ipv4first src/app.js",
    "dev": "node --watch src/app.js",
    "dev:dns": "node --dns-result-order=ipv4first --watch src/app.js",
    "test": "node --test tests/*.test.js"
  },
  "dependencies": {
    "binance-api-node": "^0.13.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "yahoo-finance2": "^3.13.0"
  }
}
